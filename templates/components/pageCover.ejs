<div class="page-cover parallax-wrapper">
    <div class="page-cover__image onload parallax"
        style="background-image:url('<%= pageResources.imageCover ? pageResources.imageCover : '' %>')"></div>
    <div class="page-cover__overlay"></div>
    <div class="page-cover__title">
        <a href="/<%= url.controller %><%= url.subPage ? '/' + url.subPage : '' %>" class="fadeout-page">
            <h1><%= pageResources.name[getLocale] %></h1>
        </a>
    </div>
</div>