<meta property="og:image"
    content="<%= pageResources.imageCover ? url.protocol + '://' + url.host + pageResources.imageCover : url.protocol + '://' + url.host + '/static/logo-big.png' %>" />

<title><%= pageMeta.title ? pageMeta.title : '' %> - <%= url.currentPage !== 'home' ? translation.DEJAVU : ''  %>
</title>

