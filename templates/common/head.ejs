<meta name="author" content="Radoslav Lazarov">

<meta property="og:url" content="<%= url.fullUrl %>" />
<meta property="og:type" content="website" />
<meta property="og:title"
    content="<%= pageMeta.title ? pageMeta.title : '' %> - <%= url.currentPage !== 'home' ? translation.DEJAVU : ''  %>" />
